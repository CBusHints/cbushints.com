{
  "name": "cbushints.com",
  "description": "",
  "version": "1.0.0",
  "homepage": "https://cbushints.com",
  "author": "Matthew Miller <matthew@mi11er.net> (https://mi11er.net)",
  "contributors": ["Jonathan Briehl"],
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/cbushints/cbushints.com.git"
  },
  "bugs": {
    "url": "https://github.com/cbushints/cbushints.com/issues"
  },
  "scripts": {
    "build": "babel-node lib/build.js --buildOnly",
    "clean": "rimraf dist firebase-debug.log npm-debug.log",
    "deploy": "run-s clean build deploy:firebase",
    "deploy:firebase": "firebase deploy",
    "lint": "eslint .",
    "lint:fix": "run-s 'lint -- --fix'",
    "start": "babel-node lib/build.js"
  },
  "dependencies": {
    "babel-cli": "6.23.0",
    "babel-preset-env": "1.1.9",
    "browser-sync": "2.18.8",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-import": "2.2.0",
    "firebase-tools": "3.3.0",
    "metalsmith": "2.3.0",
    "metalsmith-layouts": "1.8.0",
    "metalsmith-sass": "1.4.0",
    "minimist": "1.2.0",
    "npm-run-all": "4.0.2",
    "picnic": "6.3.2",
    "pug": "2.0.0-beta9",
    "rimraf": "2.6.1"
  },
  "devDependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [[
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]]
  }
}
